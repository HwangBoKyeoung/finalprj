<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper
	namespace="com.third.prj.performanceschedule.mapper.PerformanceScheduleMapper">
	<update id="PSchedulUpdate" parameterType="com.third.prj.performanceschedule.service.PerformanceScheduleVO">
		UPDATE PERFORMANCE_SCHEDULE
		<set>
			<if test="frDt != null">FRDT = #{frDt},</if>
			<if test="no != null">NO = #{no},</if>
			<if test="time != null">TIME = #{time},</if>
		</set>
		WHERE P_SCH_NO = #{pSchNo}
	</update>
</mapper>