<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.third.prj.inquiry.service.InquiryMapper">
	<select id="inquirySelectList" resultType="com.third.prj.inquiry.vo.InquiryVO">
		SELECT * FROM INQUIRY
	</select>
	<select id="inquirySelect" resultType="com.third.prj.inquiry.vo.InquiryVO">
		SELECT * FROM INQUIRY
		WHERE IN_NO = #{inNo}
	</select>
</mapper>