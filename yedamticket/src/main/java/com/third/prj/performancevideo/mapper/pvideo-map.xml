<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.third.prj.performancevideo.mapper.PerformanceVideoMapper">
	<select id="pervSelect" resultType="com.third.prj.performancevideo.service.PerformanceVideoVO">
	SELECT P_VIDEO_NO,VNAME,VRENAMES,P_NO,FILE_CD
	FROM PERFORMANCE_VIDEO
	WHERE P_NO = #{PNo}
	</select>
		
</mapper>